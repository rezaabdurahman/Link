{
  "name": "link-ios-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Link - Connect with people around you who share your interests",
  "scripts": {
    "start": "vite --mode development",
    "dev": "vite --mode development",
    "dev:local": "vite --mode development",
    "dev:staging": "vite --mode staging",
    "dev:demo": "vite --mode demo",
    "preview": "vite preview",
    "build": "npm run type-check && vite build --mode production",
    "build:development": "npm run type-check && vite build --mode development",
    "build:test": "vite build --mode test",
    "build:staging": "npm run type-check && vite build --mode staging",
    "build:production": "npm run type-check && vite build --mode production",
    "build:demo": "vite build --mode demo",
    "build:analyze": "ANALYZE=true npm run build",
    "build:report": "npm run build && npx rollup-plugin-visualizer dist/stats.html --open",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 200",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "lint:check": "npm run lint && npm run type-check",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=\"__tests__\" --testPathIgnorePatterns=\"integration\"",
    "test:integration": "jest --testPathPattern=\"integration\"",
    "test:ci": "jest --config=jest.config.ci.js --coverage --watchAll=false --ci --passWithNoTests",
    "test:snapshots": "jest --updateSnapshot",
    "test:env": "node -e \"require('dotenv').config({path:'.env.test'}); console.log('âœ… .env.test loaded with', Object.keys(process.env).filter(k=>k.startsWith('VITE_')||k==='NODE_ENV').length, 'variables'); process.exit(0)\"",
    "test:e2e": "cypress run --headless",
    "test:e2e:dev": "cypress open",
    "test:e2e:ci": "cypress run --config=cypress.config.ci.ts --headless --browser chrome",
    "test:visual": "percy exec -- cypress run --headless",
    "test:visual:dev": "percy exec -- cypress open",
    "test:visual:ci": "percy exec --quiet -- cypress run --headless --env percy=true",
    "audit:contrast": "node scripts/contrast-audit.js",
    "audit:a11y": "npm run lint",
    "audit:full": "npm run audit:contrast && npm run audit:a11y",
    "generate-icons": "node scripts/generate-icons.js",
    "ios:build": "npm run build && npx cap copy ios",
    "ios:open": "npx cap open ios",
    "ios:run": "npm run ios:build && npx cap run ios",
    "ios:prepare": "./scripts/prepare-ios-deployment.sh",
    "postinstall": "msw init public --save",
    "prepare": "husky install"
  },
  "dependencies": {
    "@capacitor/core": "7.4.2",
    "@capacitor/ios": "7.4.2",
    "@capacitor/splash-screen": "7.0.2",
    "@capacitor/status-bar": "7.0.2",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "5.2.1",
    "@sentry/integrations": "7.120.4",
    "@sentry/react": "7.120.4",
    "cross-fetch": "4.1.0",
    "date-fns": "^4.1.0",
    "framer-motion": "12.23.12",
    "jwt-decode": "^4.0.0",
    "lucide-react": "0.294.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.62.0",
    "react-icons": "5.5.0",
    "react-markdown": "10.1.0",
    "react-router-dom": "6.8.0",
    "rehype-raw": "7.0.0",
    "remark-gfm": "4.0.1",
    "swr": "2.3.6",
    "web-vitals": "4.2.4",
    "zod": "4.0.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.10.2",
    "@capacitor/cli": "^7.4.2",
    "@mswjs/interceptors": "^0.39.6",
    "@percy/cli": "^1.31.1",
    "@percy/cypress": "^3.1.6",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "cypress": "^14.5.4",
    "dotenv": "^17.2.1",
    "eslint": "^8.55.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "mock-socket": "^9.3.1",
    "msw": "^2.10.5",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwind-scrollbar-hide": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.4.19",
    "vite-bundle-analyzer": "^1.2.1",
    "vite-plugin-compression": "^0.5.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
